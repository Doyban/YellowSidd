<?xml version='1.0' encoding='utf-8'?>
<widget id="com.doyban.yellowsidd" version="3.8.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>YellowSidd</name>
  <description>YellowSidd gives you ability to play good old style platformer type game.</description>
  <author email="contact@doyban.com" href="https://doyban.com/">Doyban</author>
  <content src="index.html" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
  <plugin name="cordova-plugin-x-socialsharing" spec="^5.4.3" />
  <plugin name="cc.fovea.cordova.purchase" spec="^8.1.1" />
  <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
  <plugin name="cordova-plugin-admob-simple" spec="^3.3.8" />
  <plugin name="cordova-plugin-statusbar" spec="^2.4.2">
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="blackopaque" />
  </plugin>
  <plugin name="cordova-plugin-facebook4" spec="^5.0.0">
    <params>
      <param name="APP_ID" value="242231026115859" />
      <param name="APP_NAME" value="YellowSidd" />
    </params>
  </plugin>
</widget>
